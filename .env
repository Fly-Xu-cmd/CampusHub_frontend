# 生产环境 API 基础 URL
VITE_PROD_BASE_URL=https://api.yourdomain.com

# 应用名称
VITE_APP_NAME=CampusHub

# 版本号
VITE_APP_VERSION=1.0.0

# 是否开启调试模式
VITE_DEBUG=true

